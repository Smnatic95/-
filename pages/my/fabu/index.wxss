/* pages/my/fabu.wxss */
page {
  background-color: #F7F8F9;
}

.gList{
  background-color: #fff;
}

.gitem {
  padding-bottom: 15rpx;
  width: 95%;
  margin: 0 auto;
  border-bottom: 1px solid #eee;
}

.gitem:last-of-type{
  border: none;
}

.gitem .funcs {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.gitem .funcs .func {
  padding: 0 30rpx;
  height: 70rpx;
  line-height: 70rpx;
  border: 1px solid #eee;
  margin: 0 10rpx;
  border-radius: 50rpx;
  font-weight: 500;
}

.gitem .funcs .func.moreOpe {
  padding: 0;
  width: 70rpx;
  text-align:center;
  border-radius: 50%;
}

.gitem .funcs .func.moreOpe .iconfont{
  font-size: 40rpx;
}